{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/build/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
} 